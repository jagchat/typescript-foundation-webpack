{
  "name": "typescript-foundation-webpack",
  "version": "0.0.1",
  "description": "Trying to bundle Foundation and jQuery with Webpack in TypeScript 2.x",
  "main": "index.js",
  "scripts": {
    "lite-dev": "lite-server -c config/lite.dev.json",
    "lite-test": "lite-server -c config/lite.test.json",
    "dev-bundle": "webpack --watch --config config/webpack.dev.js",
    "test-bundle": "webpack --watch --config config/webpack.test.js && npm run lite-test",
    "prod-bundle": "rimraf dist && webpack --config config/webpack.prod.js --progress --profile --bail",
    "test": "karma start ./config/karma.conf.js"
  },
  "keywords": [
    "typescript",
    "webpack",
    "foundation"
  ],
  "author": "Der-Alex",
  "license": "GPL-3.0",
  "private": true,
  "dependencies": {
    "css-loader": "^0.25.0",
    "foundation-sites": "^6.2.3",
    "jquery": "^2.1.0",
    "lodash": "^4.16.4",
    "node-sass": "^3.10.1",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "ts-loader": "^0.9.1",
    "typescript": "^2.0.3",
    "webpack": "^1.13.2"
  },
  "devDependencies": {
    "@types/foundation-sites": "^6.1.28",
    "@types/jasmine": "^2.5.37",
    "@types/jquery": "^2.0.33",
    "@types/lodash": "^4.14.37",
    "@types/node": "^6.0.46",
    "@types/webpack": "^1.12.35",
    "core-js": "^2.4.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.24.1",
    "istanbul-instrumenter-loader": "^1.0.0",
    "jasmine-core": "^2.5.2",
    "json-loader": "^0.5.4",
    "karma": "^1.3.0",
    "karma-cli": "^1.0.1",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.8.0",
    "lite-server": "^2.2.2",
    "null-loader": "^0.1.1",
    "phantomjs-prebuilt": "^2.1.13",
    "raw-loader": "^0.5.1",
    "remap-istanbul": "^0.7.0",
    "rimraf": "^2.5.4",
    "ts-helpers": "^1.1.2",
    "webpack-merge": "^0.15.0"
  }
}
